<html>
  <head>
    <title>Mini Message Board</title>
  </head>
  <body>
    <header><%- include('navbar', { links }) %></header>
    <% messages.forEach(msg => { %>
    <div>
      <p><%= msg.id %></p>
      <p><%= msg.name %></p>
      <p><%= msg.message %></p>
      <p><%= msg.date %></p>
      <a href="/<%= msg.id %>"><button>Open</button></a>
    </div>
    <% }) %>
  </body>
</html>
